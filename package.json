{
  "name": "fisica1",
  "version": "1.0.0",
  "description": "Projecto de Universidad",
  "main": "",
  "license": "ISC",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "run-p dev:* start",
    "dev:client": "run-s lint build:client",
    "start": "webpack-dev-server --config webpack.client.js --hot --disable-host-check",
    "build:client": "webpack --config webpack.client.js",
    "prebuild": "rimraf ./public && mkdir public",
    "build": "run-s prebuild lint \"build:client -p --env=production\" \"copy-assets --subfolder=public\" jest",
    "copy-assets": "node scripts/replace-paths-static-files.js && ts-node scripts/copy-assets.ts",
    "pretty": "prettier --write \"src/client/**/*.{ts,tsx,js}\"",
    "lint": "tsc --noEmit && yarn run lint:src",
    "lint-check": "eslint --print-config src/client/App.tsx | eslint-config-prettier-check",
    "lint:src": "eslint \"src/client/**/*.{ts,tsx,js}\" --quiet --fix",
    "stylelint:src": "stylelint \"./src/**/*.css\"",
    "test": "run-s dev:client jest",
    "jest": "jest --colors",
    "postbuild": "ts-node ./scripts/react-snap.ts",
    "analyze": "source-map-explorer 'public/*.js'",
    "run:analyze": "run-s prebuild \"build:client -- -p --env=production --devtool=source-map\" \"analyze\"",
    "update:articles": "ts-node ./scripts/update-articles.ts"
  },
  "author": "",
  "dependencies": {
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "autocompleter": "^6.0.1",
    "axios": "^0.19.1",
    "core-js": "^3.6.4",
    "flat": "^5.0.0",
    "i": "^0.3.6",
    "identity-obj-proxy": "^3.0.0",
    "lozad": "^1.14.0",
    "q-flat": "^1.0.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-gtm-module": "^2.0.8",
    "react-helmet-async": "^1.0.4",
    "react-html-parser": "^2.0.2",
    "react-paginate": "^6.3.2",
    "react-router-dom": "^5.1.2",
    "react-sticky": "^6.0.3",
    "tiny-slider-react-wrapper": "^0.4.3",
    "unflatten": "^1.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/lozad": "^1.9.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.4",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-paginate": "^6.2.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-sticky": "^6.0.3",
    "@types/react-sticky-el": "^1.0.2",
    "@types/shelljs": "^0.8.5",
    "@types/use-resize-observer": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.3",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.19.1",
    "react-snap": "^1.23.0",
    "replace-in-file": "^5.0.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "shelljs": "^0.8.3",
    "source-map-explorer": "^2.4.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.8",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.1.0"
  },
  "optionalDependencies": {
    "@types/vfile-message": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache --cache-location \".cache/\"",
      "jest --colors --bail --findRelatedTests",
      "git add"
    ],
    "{config/paths,webpack.base,tsconfig,jest.config,.eslintrc,.babelrc}*.js*": [
      "node scripts/check-paths.js"
    ]
  }
}
